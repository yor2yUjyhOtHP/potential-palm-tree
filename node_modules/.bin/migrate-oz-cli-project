#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.2.0_@nomicfoundation+hardhat-ethers@3.0.6_ethers@6.13.1_hard_5me3qdrewgwt4bxcyhnvzmikyi/node_modules/@openzeppelin/hardhat-upgrades/dist/scripts/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.2.0_@nomicfoundation+hardhat-ethers@3.0.6_ethers@6.13.1_hard_5me3qdrewgwt4bxcyhnvzmikyi/node_modules/@openzeppelin/hardhat-upgrades/dist/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.2.0_@nomicfoundation+hardhat-ethers@3.0.6_ethers@6.13.1_hard_5me3qdrewgwt4bxcyhnvzmikyi/node_modules/@openzeppelin/hardhat-upgrades/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.2.0_@nomicfoundation+hardhat-ethers@3.0.6_ethers@6.13.1_hard_5me3qdrewgwt4bxcyhnvzmikyi/node_modules/@openzeppelin/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.2.0_@nomicfoundation+hardhat-ethers@3.0.6_ethers@6.13.1_hard_5me3qdrewgwt4bxcyhnvzmikyi/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.2.0_@nomicfoundation+hardhat-ethers@3.0.6_ethers@6.13.1_hard_5me3qdrewgwt4bxcyhnvzmikyi/node_modules/@openzeppelin/hardhat-upgrades/dist/scripts/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.2.0_@nomicfoundation+hardhat-ethers@3.0.6_ethers@6.13.1_hard_5me3qdrewgwt4bxcyhnvzmikyi/node_modules/@openzeppelin/hardhat-upgrades/dist/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.2.0_@nomicfoundation+hardhat-ethers@3.0.6_ethers@6.13.1_hard_5me3qdrewgwt4bxcyhnvzmikyi/node_modules/@openzeppelin/hardhat-upgrades/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.2.0_@nomicfoundation+hardhat-ethers@3.0.6_ethers@6.13.1_hard_5me3qdrewgwt4bxcyhnvzmikyi/node_modules/@openzeppelin/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@openzeppelin+hardhat-upgrades@3.2.0_@nomicfoundation+hardhat-ethers@3.0.6_ethers@6.13.1_hard_5me3qdrewgwt4bxcyhnvzmikyi/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@openzeppelin/hardhat-upgrades/dist/scripts/migrate-oz-cli-project.js" "$@"
else
  exec node  "$basedir/../@openzeppelin/hardhat-upgrades/dist/scripts/migrate-oz-cli-project.js" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/solidity-coverage@0.8.12_hardhat@2.22.6_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4__typescript@5.5.4_/node_modules/solidity-coverage/plugins/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/solidity-coverage@0.8.12_hardhat@2.22.6_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4__typescript@5.5.4_/node_modules/solidity-coverage/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/solidity-coverage@0.8.12_hardhat@2.22.6_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4__typescript@5.5.4_/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/solidity-coverage@0.8.12_hardhat@2.22.6_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4__typescript@5.5.4_/node_modules/solidity-coverage/plugins/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/solidity-coverage@0.8.12_hardhat@2.22.6_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4__typescript@5.5.4_/node_modules/solidity-coverage/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/solidity-coverage@0.8.12_hardhat@2.22.6_ts-node@10.9.2_@types+node@20.14.12_typescript@5.5.4__typescript@5.5.4_/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../solidity-coverage/plugins/bin.js" "$@"
fi

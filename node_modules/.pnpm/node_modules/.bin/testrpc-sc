#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/potential-palm-tree/node_modules/.pnpm/ghost-testrpc@0.0.2/node_modules/ghost-testrpc/node_modules:/workspaces/potential-palm-tree/node_modules/.pnpm/ghost-testrpc@0.0.2/node_modules:/workspaces/potential-palm-tree/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/potential-palm-tree/node_modules/.pnpm/ghost-testrpc@0.0.2/node_modules/ghost-testrpc/node_modules:/workspaces/potential-palm-tree/node_modules/.pnpm/ghost-testrpc@0.0.2/node_modules:/workspaces/potential-palm-tree/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ghost-testrpc/index.js" "$@"
else
  exec node  "$basedir/../ghost-testrpc/index.js" "$@"
fi
